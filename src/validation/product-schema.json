{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "locationId": {
      "type": "integer"
    },
    "sku": {
      "type": "string"
    },
    "numberPlayers": {
      "type": "integer"
    },
    "price": {
      "type": "integer"
    },
    "sportId": {
      "type": "integer"
    },
    "isCovered": {
      "type": "boolean"
    },
    "features": {
      "type": "string"
    },
    "currencyId": {
      "type": "integer"
    },
    "isActive": {
      "type": "boolean"
    },
    "images": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer"
            },
            "productId": {
              "type": "integer"
            },
            "image": {
              "type": "string"
            },
            "isActive": {
              "type": "boolean"
            }
          },
          "required": [
            "id",
            "productId",
            "image",
            "isActive"
          ]
        }
      ]
    }
  },
  "required": [
    "id",
    "locationId",
    "sku",
    "numberPlayers",
    "price",
    "sportId",
    "isCovered",
    "features",
    "currencyId",
    "images",
    "isActive"
  ]
}